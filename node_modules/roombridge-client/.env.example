# RoomBridge Client Environment Variables
# Copy this file to .env and fill in your actual values

# API Configuration
REACT_APP_API_URL=https://your-backend-domain.vercel.app

# Telegram Configuration
REACT_APP_TELEGRAM_BOT_USERNAME=your-bot-username

# TON Configuration (if needed)
REACT_APP_TON_MANIFEST_URL=https://your-domain.com/tonconnect-manifest.json

# Analytics (optional)
REACT_APP_VERCEL_ANALYTICS_ID=your-analytics-id
